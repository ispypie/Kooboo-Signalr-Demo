@model IEnumerable<ChatModule.Models.News>
@{
    Layout = "~/Areas/" + ChatModule.Areas.ChatModule.ModuleAreaRegistration.ModuleName + "/Views/Shared/Front.cshtml";
}
<div class="block menu">
    <h2>@("Lastest news from module".Label())</h2>
    @if (Model != null)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    @Html.ModuleHtml().ActionLink(item.Title, "Detail", new { id = item.Id })
                </li>
            }
        </ul>
    }
</div>
